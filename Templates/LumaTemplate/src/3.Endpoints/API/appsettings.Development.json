{
  "ApplicationName": "LumaTemplate",
  "ServiceId": "LumaTemplate",
  "ServiceName": "LumaTemplate",
  "ServiceVersion": "1.0.0",

  "ConnectionStrings": {
    "CommandDb_ConnectionString": "Server=localhost,1433;Database=LumaTemplateDb;User Id=sa;Password=Luma@Containers;TrustServerCertificate=True;  MultipleActiveResultSets=true;",
    "QueryDb_ConnectionString": "Server=localhost,1433;Database=LumaTemplateDb;User Id=sa;Password=Luma@Containers;TrustServerCertificate=True;  MultipleActiveResultSets=true;"

  },

  "Cache": {
    "TableName": "Caching",
    "SchemaName": "dbo",
    "ConnectionString": "Server=localhost,1433;Database=CacheDb;User Id=sa;Password=Luma@Containers;TrustServerCertificate=True;  MultipleActiveResultSets=true;",
    "AutoCreateSqlTable": true
  },

  "spring": {
    "application": {
      "name": "LumaTemplate"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": "false",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "instance": {}
  },

  "PollingPublisher": {
    "ApplicationName": "LumaTemplate",
    "SendCount": 100,
    "SendInterval": 60000,
    "ExceptionInterval": 10000
  },
  "PollingPublisherSqlStore": {
    "ConnectionString": "Server=localhost,1433;Database=LumaTemplateDb;User Id=sa;Password=Luma@Containers;TrustServerCertificate=True;  MultipleActiveResultSets=true;",
    "ApplicationName": "LumaTemplate"
  },

  "MessageInbox": {
    "ApplicationName": "LumaTemplate"
  },
  "MessageInboxSqlStore": {
    "AutoCreateSqlTable": true,
    "ConnectionString": "Server=localhost,1433;Database=LumaTemplateDb;User Id=sa;Password=Luma@Containers;TrustServerCertificate=True;  MultipleActiveResultSets=true;",
    "SchemaName": "dbo",
    "TableName": "MessageInbox"
  },

  "RabbitMq": {
    "PerssistMessage": true,
    "ExchangeName": "LumaTemplateExchange",
    "ServiceName": "LumaTemplate",
    "Url": "amqp://admin:admin@localhost:5672/"
  },

  "AutoMapper": {
    "AssemblyNamesForLoadProfiles": "LumaTemplate"
  },

  "ParrotTranslator": {
    "AutoCreateSqlTable": true,
    "ConnectionString": "Server=localhost,1433;Database=LumaTemplateDb;User Id=sa;Password=Luma@Containers;TrustServerCertificate=True;  MultipleActiveResultSets=true;",
    "SchemaName": "dbo",
    "TableName": "ParrotTranslations",
    "ReloadDataIntervalInMinuts": 60,
    "DefaultTranslations": [
      {
        "Key": "Luma",
        "Value": "لوما",
        "Culture": "fa-IR"
      },
      {
        "Key": "Luma",
        "Value": "Luma",
        "Culture": "en-US"
      }
    ]
  },

  "WebUserInfo": {
    "DefaultUserId": -1
  },

  "Scalar": {
    "Enabled": true,
    "Title": "LumaTemplate Scalar",
    "Layout": "Classic", // Classic, Modern
    "Theme": "DeepSpace", // Alternate, Default, Moon, Purple, Solarized, BluePlanet, Saturn, Kepler, Mars, DeepSpace, Laserwave, None
    "EndpointPrefix": "/",
    "ClientCredentials": {
      "Enabled": true,
      "ClientId": "Scalar",
      "ClientSecret": "p4%/4onZ]bg9",
      "SelectedScopes": [ "LumaTemplate" ]
    }
  },

  "SoftwarePart": {
    "ApplicationName": "UnknownSoftware",
    "ModuleName": "UnknownModule",
    "ServiceName": "UnknownService",
    "DestinationServiceBaseAddress": "https://localhost:7014/",
    "DestinationServicePath": "api/SoftwarePart/CreateByDetector",
    "FakeSSL": false,
    "OAuth": {
      "Enabled": false,
      "Authority": "https://localhost:5000",
      "ClientId": "softwarepart-detector",
      "ClientSecret": "secret"
    }
  },

  "ApiAuthentication": {
    "Enabled": false,
    "Providers": [
      {
        "IgnoreSSL": true,
        "Scheme": "LumaDuendeIdp",
        "Authority": "https://localhost:5001",
        "JwtTokenConfigOption": 1,
        "Priority": 1
      }
    ]
  },

  "OpenTelemetry": {
    "ApplicationName": "LumaTemplate",
    "ServiceName": "LumaTemplate",
    "ServiceId": "LumaTemplate",
    "ServiceVersion": "1.0.0",
    "OtlpEndpoint": "http://localhost:4317",
    "ExportProcessorType": "Simple",
    "SamplingProbability": 1
  },

  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Luma": "Verbose"
      }
    },
    "Enrich": [ "FromLogContext", "WithExceptionDetails", "WithSpan" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "%TEMP%\\Logs\\LumaTemplate.Log.txt" }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "indexFormat": "Luma-LumaTemplate-serilog-index-{0:yyyy.MM}",
          "restrictedToMinimumLevel": "Verbose",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv6",
          "batchPostingLimit": 50,
          "period": 2,
          "inlineFields": true,
          "bufferFileSizeLimitBytes": 5242880,
          "bufferLogShippingInterval": 5000,
          "bufferRetainedInvalidPayloadsLimitBytes": 5000,
          "bufferFileCountLimit": 31,
          "connectionTimeout": 5,
          "emitEventFailure": "WriteToSelfLog",
          "queueSizeLimit": "100000"
        }
      }
    ]
  },

  "AllowedHosts": "*"
}
