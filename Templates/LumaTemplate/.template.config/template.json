{
    "$schema": "http://json.schemastore.org/template",
    "author": "Luma",
    "classifications": [ "Luma", "DDD", "Web API", "API" , "Solution" ],
    "identity": "Luma.Default.SolutionTemplate.CSharp",
    "name": "Luma Blank Solution",
    "shortName": "Lumasln",
    "sourceName": "LumaTemplate",
    "preferNameDirectory": true,
    "tags": {
        "language": "C#",
        "type": "solution"
    },
    "symbols": {
        "namespace": {
            "type": "parameter",
            "replaces": "LumaTemplate",
            "FileRename": "LumaTemplate",
            "defaultValue": "LumaTemplate"
        },
        "dbContextName": {
            "type": "parameter",
            "replaces": "DbContextName",
            "FileRename": "DbContextName",
            "defaultValue": "DbContextName"
        },
        "httpPort": {
            "type": "parameter",
            "replaces": "5198",
            "defaultValue": "5198",
            "description": "HTTP port number"
        },
        "httpsPort": {
            "type": "parameter",
            "replaces": "7202",
            "defaultValue": "7202",
            "description": "HTTPS port number"
        },
        "environment": {
            "type": "parameter",
            "replaces": "EnvironmentValue",
            "defaultValue": "Development",
            "description": "ASP.NET Core environment"
        },
        "httpProfileName": {
            "type": "parameter",
            "replaces": "HttpProfileName",
            "defaultValue": "http",
            "description": "Name for the HTTP launch profile"
        },
        "httpsProfileName": {
            "type": "parameter",
            "replaces": "HttpsProfileName",
            "defaultValue": "https",
            "description": "Name for the HTTPS launch profile"
        }
    }
}